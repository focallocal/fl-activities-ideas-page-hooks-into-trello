<section id="{{id}}" class="section">
  <div class="row">
    <a name="{{id}}"></a>
    <div class="col m12 s12 l8 offset-l2">
      {{#bigVideo}}
      <p class="youtube-video">
        <iframe class="youtube-video" width="100%" height="500" src="https://www.youtube.com/embed/{{youtubeId}}" frameborder="0" allowfullscreen></iframe>
      </p>
      {{/bigVideo}}

      {{#title}}
        <div class="strike">
          <h2><span>{{title}}</span></h2>
        </div>
      {{/title}}

      {{#quote}}
        {{> quote}}
      {{/quote}}

      <p>{{{content}}}<p>

      <div class="cards-container">
        <div class="row">
          {{#cards}}
            <div class="col">{{> card }}</div>
          {{/cards}}
        </div>
      </div>


    </div>


    <div class="row small-video-container">
    {{#videos}}
        <div class="small-video">
          <h4>{{title}}</h4>
          <iframe class="youtube-video" width="100%" height="200" src="https://www.youtube.com/embed/{{youtubeId}}" frameborder="0" allowfullscreen></iframe>
        </div>
    {{/videos}}
    </div>

</section>
